;; @@ [clj]
(def ping-server
  (future 
    (zmq/with-new-context
      (let [socket (zmq/socket :rep {:bind "tcp://127.0.0.1:4321"})]
        (while true 
          (let [msg (zmq/receive-msg socket)]
            (println "RCVD ping.")
            (zmq/send-msg socket "pong")))))))

