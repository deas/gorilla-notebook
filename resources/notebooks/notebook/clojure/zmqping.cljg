;; **
;;; # Lets Ping the server
;; **

;; @@ [clj]
(zmq/with-new-context
  (let [socket (zmq/socket :req {:connect "tcp://127.0.0.1:4321"})]
      (zmq/send-msg socket "ping")
      (let [rmsg (zmq/receive-msg socket {:stringify true})]
        (println "RCVD: " rmsg))))
